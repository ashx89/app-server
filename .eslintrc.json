{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", "tab"],
        "no-var": 0,
        "no-shadow": 0,
        "wrap-iife": ["error", "any"],
        "vars-on-top": 0,
        "no-loop-func": 0,
        "comma-dangle": 0,
        "prefer-template": 0,
        "no-param-reassign": 0,
        "object-shorthand": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "prefer-arrow-callback": 0
    }
}